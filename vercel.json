{
	"version": 2,
	"buildCommand": "npm run build",
	"devCommand": "npm run start",
	"builds": [
		{
			"src": "package.json",
			"use": "vercel-sapper"
		},
		{
			"src": "dist/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.js",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE",
				"PATCH",
				"OPTIONS"
			],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api"
		}
	]
}